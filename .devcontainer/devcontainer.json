{
    "name": "appPark",
    "dockerComposeFile": [
        "../compose.yaml",
        "docker-compose.yml"
    ],

    "service": "apiPark",
    "workspaceFolder": "/app",
    "customizations": {
        "vscode": {
            "extensions": [
                "vscjava.vscode-java-pack"
            ],
            "settings": {
                "terminal.integrated.shell.linux": "/bin/bash",
		        "java.home": "/docker-java-home"
            },
            "forwardPorts":[8080],
            "remotUser": "vscode"
        }
    }
}